[user]
  name = Trevor Royer
  email = trevorroyer@gmail.com
[core]
  editor = vi
  excludesfile = ~/.gitignore
  autocrlf = input
[credential]
  helper = cache --timeout=43200
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[init]
  defaultBranch = main
[push]
  default = current
  autoSetupRemote = true
[pull]
  rebase = true
